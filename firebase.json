{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/players/**",
      "destination": "/note-page.html"
    }, {
      "regex": "/players(/.*)?",
      "destination": "/note-page.html"
    }, {
      "source": "/characters/**",
      "destination": "/note-page.html"
    }, {
      "regex": "/characters(/.*)?",
      "destination": "/note-page.html"
    }]
  }
}