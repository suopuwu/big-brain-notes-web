{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/player/**",
      "destination": "/note-page.html"
    }, {
      "regex": "/player(/.*)?",
      "destination": "/note-page.html"
    }, {
      "source": "/character/**",
      "destination": "/note-page.html"
    }, {
      "regex": "/character(/.*)?",
      "destination": "/note-page.html"
    }]
  }
}